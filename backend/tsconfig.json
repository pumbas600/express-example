{
	"compilerOptions": {
		"sourceMap": true,
		"target": "es6",
		"module": "commonjs",
		"outDir": "./dist",
		"baseUrl": "./src",
		"strict": true,
		"alwaysStrict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules"]
}
